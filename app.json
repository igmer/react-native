{
  "expo": {
    "name": "expo-firebase-app",
    "description": "This project is really great.",
    "slug": "expo-firebase-app",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#0A48A5"
    },

    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.igmer.net"
    }
  }
}
